{
  "name": "cm.ms.gge.media",
  "version": "0.0.1",
  "description": "Generic small microservice to manipulate and save media",
  "main": "index.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "db:create:dev": "cross-env NODE_ENV=development node scripts/db/create",
    "db:delete:dev": "cross-env NODE_ENV=development node scripts/db/delete",
    "db:flush:dev": "cross-env NODE_ENV=development npm run db:delete:dev && npm run db:create:dev",
    "dev": "cross-env NODE_ENV=development nodemon",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:dev": "cross-env NODE_ENV=test jest --watchAll",
    "testWithCoverage": "cross-env NODE_ENV=test jest --coverage",
    "eslint:fix": "eslint .  --ext .ts --fix",
    "eslint": "eslint . --ext .ts --ignore-path .gitignore",
    "prepare": "husky install",
    "lint-staged": "npx lint-staged"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cloudinary": "^2.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "shallow-equal-object": "^1.1.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.13",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^5.2.1",
    "eslint": "^8.57.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^2.7.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "10.7.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IT-WIBRC/cm.ms.gge.media.git"
  },
  "keywords": [
    "Micro-service",
    "media",
    "CDN"
  ],
  "author": "Waffeu Ivany Botrel Rayn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IT-WIBRC/cm.ms.gge.media/issues"
  },
  "homepage": "https://github.com/IT-WIBRC/cm.ms.gge.media#readme"
}
